<div class="row text-center">
  <h1>Match Friends</h1>
  <% current_user.minus_friend(@selected_user).each do |user| %>
    <% if user.picture_url %>
      <%= link_to image_tag(user.picture, class:"picture") + user.name, user_matches_path(@selected_user, matched_user: user.id), :method => :post %>
    <% else %>
      <%= link_to image_tag(user.picture_url, class:"picture") + user.name, user_matches_path(@selected_user, matched_user: user.id), :method => :post %>
    <% end %>
  <% end %>
</div>
